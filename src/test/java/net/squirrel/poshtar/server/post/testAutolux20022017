    <script type="text/javascript">
        function initialize() {
            var mapOptions = {
                center: new google.maps.LatLng(48.41706600000000, 35.06025900000000),  // центрирование
                zoom: 14,
                mapTypeId: google.maps.MapTypeId.ROADMAP
            };
            var map = new google.maps.Map(document.getElementById("map-canvas"), mapOptions);

            var infowindow = new google.maps.InfoWindow({


                    content: '<div style="color: #000; text-align: justify;">' + '<img style="width: 20px; height: 20px" src="/static/img/auto.png" > &nbsp;' +
                    '<strong>' + 'Днепр (Днепропетровск)' + '</strong>' +
                    '<br />' + '<i class="fa fa-map-marker" style="color: #4A449B;"></i>&nbsp;&nbsp;' + 'пр-т Героев, 12' +
                    '<br />' + '<i class="fa fa-phone" style="color: #4A449B;"></i>&nbsp;&nbsp;' + '+38 056 785 20 01' +
                    '<br />' + '<i class="fa fa-clock-o" style="color: #4A449B;"></i>&nbsp;&nbsp;' + 'пн-пт: 08:00 - 20:00; сб: 08:00 - 19:00; вс: выходной' +
                    '</div>'

            });

            var image = new google.maps.MarkerImage('/static/img/map_marker_violet_se.png',
                    new google.maps.Size(32, 46),
                    new google.maps.Point(0, 0),
                    new google.maps.Point(0, 32)
            );

            var marker = new google.maps.Marker({
                position: new google.maps.LatLng(48.41706600000000, 35.06025900000000),  // координаты маркера
                map: map,
                icon: image,
                title: 'Офис местонахождения груза: ',
                clickable: true
            });


            google.maps.event.addListener(marker, 'click', function () {
                infowindow.open(map, marker);
            });
            infowindow.open(map, marker);
        }
        google.maps.event.addDomListener(window, 'load', initialize);
    </script>


