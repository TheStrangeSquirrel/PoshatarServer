						var err = '';
						var lang = 'ru';
						jQuery( document ).ready(function() {
							if (err == 'no_find'){
								//showMessage('', 'Відправлення не знайдено<br> Можливо це номер компанії відправника? Виберіть компанію  відправника та  повторіть пошук');
