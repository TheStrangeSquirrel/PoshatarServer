						<div class="dialog_subsc">
							<div class="sbs_title">Получить изменении статуса отправления на e-mail:</div>
							<div class="sbs_label">Номер отправления:</div>
							<input type="text" class="sbs_nomer" id="sbs_nomer">
							<div class="sbs_label">email</div>
							<input type="text" class="sbs_email" id="sbs_email">
							<div class="sbs_agree">
								<input type="checkbox" id="sbs_agree" value="1" checked>
								<label for="sbs_agree">Хочу получать другие сообщения от Meest Group на e-mail</label>
							</div>
							<div>
								<input type="button" class="button" value="Получить" onclick="subscribe()">
							</div>
						</div>
					</div>
					<div id="dlg_msg_subscr_ok" style="display:none">
						Вы успешно подписались на сообщение при изменении статуса					</div>
					<div id="dlg_msg_subscr_doubl" style="display:none">
						Вы уже подписаны на уведомления об изменении статуса					</div>
					<div id="dlg_msg_subscr_fail" style="display:none">
						Не удается подписаться на изменение статусов					</div>
					<script>
						var err = 'no_find';
						var lang = 'ru';
						jQuery( document ).ready(function() {
							if (err == 'no_find'){
								//showMessage('', 'Відправлення не знайдено<br> Можливо це номер компанії відправника? Виберіть компанію  відправника та  повторіть пошук');
							}
						})
					</script>

					<script>
						function validateEmail(email) {
							var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
							return re.test(email);
						}
						function tracking_search(){
							var send = false;
							var t_form = document.getElementById('t_form');
							if (t_company_sel != ''){
								var track_num = jQuery('#declar').val();
								track_num = track_num.replace(new RegExp('/','g'), '_');
								var path = t_company_sel.replace('t_', '') + '/';

								if (t_company_sel == 't_amway'){
									track_num = 'AM' + track_num;
								}

								path = 'https://t.meest-group.com' + '/' + path + lang + '/' + track_num;
								t_form.action = path;
								t_form.target = '_blank';
								send = true;
							} else {
								t_form.action = '';
								t_form.target = '_self';
								send = true;
								//trackingSearch();
							}

							return send;
						}

						function showSubscribe() {
							jQuery('#sbs_nomer').val(jQuery('#declar').val());
							jQuery('#dialog_subsc').dialog({
								dialogClass: 'dialog_subscribe',
								width: 530,
								height: 320,
								modal: true
							});
						}

						function subscribe(){
							jQuery('#sbs_nomer, #sbs_email').removeClass('err');
							var res = true;
							var sbs_nomer = jQuery('#sbs_nomer').val();
							var sbs_email = jQuery('#sbs_email').val();
							if (sbs_nomer == ''){
								 jQuery('#sbs_nomer').addClass('err');
								 res = false;
							}
							if (!validateEmail(sbs_email)){
								 jQuery('#sbs_email').addClass('err');
								 res = false;
							}
							if (res){
								var agent  = jQuery('#agent').val();
								if (agent == ''){
									var post_url = 'get.php?what=tracking&number=' + sbs_nomer;
								} else {
									var post_url = 'get.php?what=tracking_agent&number=' + sbs_nomer + '&agent=' + agent;
								}
								var agree  = document.getElementById('sbs_agree').checked;
								if (agree){
									post_url += '&agree=1';
								} else {
									post_url += '&agree=0';
								}
								post_url += '&out=html&subscribe&email=' + sbs_email;
								jQuery.post(post_url, {}, function(res){
									jQuery( '#dialog_subsc' ).dialog( 'close' );
									if (res == '000'){
										jQuery('#dlg_msg_subscr_ok').dialog();
									} else {
										if (res == '1') {
											jQuery('#dlg_msg_subscr_doubl').dialog();
										} else {
											jQuery('#dlg_msg_subscr_fail').dialog();
										}
									}
								})
							}
						}

						var t_selector = '#t_lamoda, #t_amway, #t_leboutique, #t_bonprix, #t_next, #t_stylepit, #t_eurobuy, #t_answear, #t_modnakasta';
						var t_company_sel = '';
						jQuery( document ).ready(function() {

							jQuery(t_selector).click( function() {
								jQuery(t_selector).removeClass('t_company_selected');
								if (this.id == t_company_sel){
									t_company_sel = '';

								} else {
									t_company_sel = this.id;
									jQuery('#' + this.id).addClass('t_company_selected');
									//tracking_search(); document.forms['t_form'].submit();
								}
							} )

							jQuery('#t_img_1').click( function() {
								//window.parent.location.href = 'https://meest-group.globalmoney.ua/?base1c=1C_Logistic#debt';
								window.parent.location.href = 'https://meest-express.com.ua/ru/services/mymeest';
							});
							jQuery('#t_img_2').click( function() {
								window.parent.location.href = 'https://meest-express.com.ua/ru/services/mgnovennye-perevody';
							});
						});

					</script>
				</div>
			</div>
	   </div>
	</div>
